{
    "providers":{
        "angaza":{
            "tables":{
                "accounts":"accounts",
                "users":"users",
                "payments":"payments",
                "replacements":"replacements",
                "stock":"stock_unit_statuses",
                "applications":"prospects",
                "clients":"clients"
            }
        },
        "upya":{
            "tables":{
                "accounts":"contracts",
                "users":"",
                "payments":"",
                "replacements":"",
                "stock":"",
                "applications":"",
                "clients":""
            }
        }
    },
    "core":{
        "accounts":{},
        "users":{},
        "payments":{
            "update_query":{
                "sql":"select {} from core.payments a where (a.effective_utc > NOW() - INTERVAL '10 DAYS' or (a.reversal_external_id is not null and a.effective_utc > NOW() - INTERVAL '60 DAYS')) and a.zoho_id is not null"
            }
        },
        "replacements":{},
        "stock":{},
        "applications":{
            "update_query":{
                "sql":"select {} from core.applications a join core.accounts b on a.account_external_id = b.account_external_id where a.submitted_utc > NOW() - INTERVAL '15 DAYS'  or (b.account_external_id is not null and b.registration_date_utc > NOW() - INTERVAL '15 DAYS') and a.zoho_id is not null"
            }
        },
        "clients":{}
    },
    "zoho":{
        "users":{
            "table":"users",
            "form":"Users_Data_Import",
            "slice_length":500
        },
        "accounts":{
            "table":"accounts",
            "form":"Accounts_Data_Import_V2",
            "slice_length":500
        },
        "payments":{
            "table":"payments",
            "form":"Payments_Data_Import",
            "slice_length":1200
        },
        "credit_details":{
            "table":"applications",
            "form":"Applications_Credit_Details",
            "slice_length":350
        },
        "personal_details":{
            "table":"applications",
            "form":"Applications_Personal_Details",
            "slice_length":1000,
            "update_ydb":false

        },
        "stock":{
            "table":"stock",
            "form":"Stock_Data_Import",
            "slice_length":1000
        },
        "replacements":{
            "table":"replacements",
            "form":"Replaced_Units_Report",
            "slice_length":1000
        },
        "clients":{
            "table":"clients",
            "form":"",
            "slice_length":1000
        }
    }
}